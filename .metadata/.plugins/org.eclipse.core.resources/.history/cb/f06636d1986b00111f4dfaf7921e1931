package broadway.kyle.html;

/**
 * @author KBroadway
 * This type of tag does not allow a 
 * public add to the renderer children.
 *
 *
 */
public abstract class TagNonComposite extends TagGeneric
{
    protected void add(String text)
    {
        this.text = new Text(text);
    }

    protected void add(Renderer toRender)
    {
        this.toRenderSet.add(toRender);
    }
}
