package broadway.kyle.html;

import java.io.PrintStream;
import java.util.HashSet;
import java.util.Set;

public class GenericTag implements Renderer
{
    protected static final NullText NULL_TEXT = new NullText();

    protected Set<Text> textSetToRender = new HashSet<Text>();
    protected Text text = NULL_TEXT;

    @Override
    public void render(PrintStream stream)
    {
        // TODO Auto-generated method stub

    }

    public GenericTag()
    {
    }

    public GenericTag(String text)
    {
        this.text = new Text(text);
    }
}
